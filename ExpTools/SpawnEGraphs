MINNEDGE=$1
MAXNEDGE=$2
NNODES=$3
DIR=$4
NUMFILES=$5

if [ ! -d $DIR ]
	then
		mkdir $DIR
fi

NEDGERANGE=$(( MAXNEDGE - MINNEDGE ))
INCREMENT=$(( NEDGERANGE/NUMFILES ))

NEDGE=$MINNEDGE
COUNT=0

npadding=${#MAXNEDGE}
while (( NEDGE <= MAXNEDGE ))
	do
		GraphGen $NNODES $NEDGE ${DIR}/expGraph$(printf "%0*d\n" $npadding $NEDGE)edges.txt
		(( NEDGE += INCREMENT ))
	done
