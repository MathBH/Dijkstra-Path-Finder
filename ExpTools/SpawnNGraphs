MINNNODE=$1
MAXNNODE=$2
NEDGE=$3
DIR=$4
NUMFILES=$5

if [ ! -d $DIR ]
	then
		mkdir $DIR
fi

NNODERANGE=$(( MAXNNODE - MINNNODE ))
INCREMENT=$(( NNODERANGE/NUMFILES ))

NNODE=$MINNNODE
COUNT=0

npadding=${#MAXNNODE}
while (( NNODE <= MAXNNODE ))
	do
		GraphGen $NNODE $NEDGE ${DIR}/expGraph$(printf "%0*d\n" $npadding $NNODE)nodes.txt
		(( NNODE += INCREMENT ))
	done
